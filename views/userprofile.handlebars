
  <div>See Below for your new character</div>
    
    
    {{#each characters as || character}}
   <div class="character-card-header">

    {{character.name}}
  </div>
  <div class="card-body">
    <p>
      Class:
      {{character.class_id}}
    </p>
    <p>
      Race:
      {{character.race}}
    </p>
    <p>
      Image:
      {{character.image}}
    </p>
    <p>
      Level:
      {{character.level}}
    </p>
    <p>
      Armor:
      {{character.armor}}
    </p>
    <p>
      Speed:
      {{character.speed}}
    </p>
    <p>
      Dexterity:
      {{character.dexterity}}
    </p>
    <p>
      Constitution:
      {{character.constitution}}
    </p>
    <p>
      Intelligence:
      {{character.intelligence}}
    </p>
    <p>
      Wisdom:
      {{character.wisdom}}
    </p>
    <p>
      Charisma:
      {{character.charisma}}
    </p>
    <p>
      Health:
      {{character.health}}
    </p>
    <p>
      Max_health:
      {{max_health}}
    </p>
    {{/each}}
  </div>
</div>

<script src="./js/profile.js"></script>
